<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.shape.Line?>
<?import javafx.scene.text.*?>
<AnchorPane fx:id="ap_main" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity"
            prefHeight="768.0" prefWidth="1080.0" xmlns="http://javafx.com/javafx/19"
            xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <ImageView fitHeight="768.0" fitWidth="1080.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@assets/background.png"/>
         </image>
      </ImageView>
      <ImageView fx:id="im_avatar" fitHeight="130.0" fitWidth="130.0" layoutX="48.0" layoutY="34.0" pickOnBounds="true"
                 preserveRatio="true">
         <image>
            <Image url="@assets/avatars/ava4.jpg"/>
         </image>
      </ImageView>
      <Label fx:id="label_nickname" alignment="CENTER" layoutX="21.0" layoutY="164.0" prefHeight="45.0"
             prefWidth="184.0" text="NickName" textFill="#fb2cff">
         <font>
            <Font name="Roboto Bold" size="19.0"/>
         </font>
      </Label>
      <Label fx:id="label_role" alignment="CENTER" layoutX="65.0" layoutY="199.0" prefHeight="21.0" prefWidth="96.0"
             text="role" textFill="WHITE">
         <font>
            <Font name="Roboto" size="12.0"/>
         </font>
      </Label>
      <Button fx:id="button_logout" layoutX="23.0" layoutY="723.0" mnemonicParsing="false"
              style="-fx-background-color: transparent; -fx-cursor: hand;" text="Logout" textFill="#f3cbc5"/>
      <Button fx:id="button_table" layoutX="49.0" layoutY="507.0" mnemonicParsing="false"
              onAction="#tableButtonOnAction" prefHeight="28.0" prefWidth="130.0"
              style="-fx-background-color: #fb2cff; -fx-background-radius: 10; -fx-cursor: hand;" text="GO TO TABLE"
              textFill="WHITE">
         <font>
            <Font name="Roboto Bold" size="14.0"/>
         </font>
      </Button>
      <ImageView fitHeight="32.0" fitWidth="28.0" layoutX="1038.0" layoutY="20.0" pickOnBounds="true"
                 preserveRatio="true" style="-fx-cursor: hand;">
         <image>
            <Image url="@assets/icons/x-button.png"/>
         </image>
      </ImageView>
      <Button fx:id="close_button" layoutX="1038.0" layoutY="20.0" mnemonicParsing="false"
              onAction="#closeButtonOnAction" prefHeight="28.0" prefWidth="28.0"
              style="-fx-background-color: TRANSPERENT; -fx-cursor: hand;"/>
      <Pane fx:id="pane_map" layoutX="278.0" layoutY="89.0" prefHeight="562.0" prefWidth="760.0"
            style="-fx-background-color: white; -fx-border-color: black;"/>
      <Label fx:id="label_all" alignment="CENTER" layoutX="277.0" layoutY="52.0" prefHeight="36.0" prefWidth="66.0"
             text="All(0)" textFill="#343a40">
         <font>
            <Font name="Roboto" size="15.0"/>
         </font>
      </Label>
      <MenuButton fx:id="mb_language" layoutX="142.0" layoutY="723.0" mnemonicParsing="false"
                  style="-fx-background-color: transperent; -fx-cursor: hand;" text="Language" textFill="#f3cbc5">
         <items>
            <MenuItem fx:id="mi_english" mnemonicParsing="false" text="English"/>
            <MenuItem fx:id="mi_russian" mnemonicParsing="false" text="Русский"/>
            <MenuItem fx:id="mi_belorussian" mnemonicParsing="false" text="Беларускі"/>
            <MenuItem fx:id="mi_spanish" mnemonicParsing="false" text="Español"/>
         </items>
      </MenuButton>
      <Button fx:id="button_info" layoutX="25.0" layoutY="247.0" mnemonicParsing="false" onAction="#infoButtonOnAction"
              prefHeight="30.0" prefWidth="175.0" style="-fx-background-color: transparent;" text="INFO"
              textFill="WHITE">
         <font>
            <Font name="Roboto Bold" size="16.0"/>
         </font>
      </Button>
      <Line endX="89.0" layoutX="110.0" layoutY="237.0" startX="-84.0" stroke="#fb2cff" strokeWidth="2.0"/>
      <Line endX="89.0" layoutX="111.0" layoutY="400.0" startX="-84.0" stroke="#fb2cff" strokeWidth="2.0"/>
      <Label fx:id="label_users" layoutX="34.0" layoutY="288.0" text="USERS:" textFill="WHITE">
         <font>
            <Font name="Roboto Bold" size="14.0"/>
         </font>
      </Label>
      <Label fx:id="label_humanbeing" layoutX="34.0" layoutY="340.0" text="HUMANBEING:" textFill="WHITE">
         <font>
            <Font name="Roboto Bold" size="14.0"/>
         </font>
      </Label>
      <Label fx:id="label_number_users" layoutX="174.0" layoutY="288.0" text="0" textAlignment="CENTER"
             textFill="WHITE">
         <font>
            <Font name="Roboto Bold" size="14.0"/>
         </font>
      </Label>
      <Label fx:id="label_nuber_humanbeing" layoutX="174.0" layoutY="340.0" text="0" textFill="WHITE">
         <font>
            <Font name="Roboto Bold" size="14.0"/>
         </font>
      </Label>
   </children>
</AnchorPane>
